<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pibigstar.demo">

    <!-- 二维码需要的权限-->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.pibigstar.demo.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pibigstar.demo.activity.GridViewActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.TabBarActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.ZxingCodeActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.LoopImageActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.DivergeViewActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.DanMuActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.CityPickerWebViewActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.DynamicGridViewActivity"></activity>
        <activity android:name="com.pibigstar.demo.activity.PhotoBrowseActivity"></activity>

    </application>

</manifest>